<% if @users.present? %>

    <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name%></td>
      <td><%= user.last_name%></td>
      <td><%= user.email%></td>
      <td><%= user.roles.map(&:name).join(", ")%></td>
    </tr>
<% end %>
<% else %>
    <tr><td style="text-align: center" colspan="3">No Data Available</td></tr>
<% end %>